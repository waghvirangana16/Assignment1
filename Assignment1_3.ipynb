    "#count of odd and even numbers\n",
    "x=list(input(\"Give a series of Numbers \"))\n",
    "a=0\n",
    "b=0\n",
    "for i in x:\n",
    "    if(int(i)%2==0):\n",
    "        a=a+1\n",
    "    else:\n",
    "        b=b+1\n",
    "print(\"Number of even numbers: \",a)\n",
    "print(\"Number of even numbers: \",b)"
